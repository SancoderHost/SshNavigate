        android:textAlignment="center"
        android:textColor="#FFEB3B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.563" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="426dp"
        android:layout_height="41dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="9dp"
        android:background="#673AB7"
        android:text="Device connected "
        android:textAlignment="center"
        android:textColor="#FFEB3B"
        app:layout_constraintBottom_toTopOf="@+id/usb_connected"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView32" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="30dp"
        android:layout_height="27dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/temp_new"
        app:layout_constraintBottom_toTopOf="@+id/imageView25"
        app:layout_constraintEnd_toStartOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="30dp"
        android:layout_height="27dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/clock_speed_new"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/cpu_name_use"
        app:srcCompat="@drawable/clock_speed_new" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="30dp"
        android:layout_height="27dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/temp_new"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/gpu_temp_use"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gpu_name_use"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/temp_new" />

    <ProgressBar
        android:id="@+id/Ramprogress"
